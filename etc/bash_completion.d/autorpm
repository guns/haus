# autorpm(8) completion

have autorpm || return

_autorpm()
{
    local cur prev words cword
    _init_completion || return

    COMPREPLY=( $( compgen -W '--notty --debug --help --version auto add \
        fullinfo info help install list remove set' -- "$cur" ) )

} &&
complete -F _autorpm autorpm

