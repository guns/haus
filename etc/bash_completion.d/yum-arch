# yum-arch(8) completion

have yum-arch || return

_yum_arch()
{
    local cur prev words cword
    _init_completion || return

    if [[ "$cur" == -* ]] ; then
        COMPREPLY=( $( compgen -W '-d -v -vv -n -c -z -s -l -q' -- "$cur" ) )
    else
        _filedir -d
    fi
} &&
complete -F _yum_arch yum-arch

