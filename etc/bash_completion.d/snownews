# snownews(1) completion

have snownews || return

_snownews()
{
    local cur prev words cword
    _init_completion || return

    if [[ "$cur" == -* ]]; then
        # return list of available options
        COMPREPLY=( $( compgen -W '$( _parse_help "$1" )' -- "$cur" ) )
    fi
} &&
complete -F _snownews snownews

