<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">

<menu id="root-menu" label="Openbox 3">
  <item label="rxvt-unicode">
    <action name="Execute">
      <command>rxvt-unicode --client</command>
    </action>
  </item>
  <item label="tmux + gpg">
    <action name="Execute">
      <command>rxvt-unicode --client -e tmuxlaunch -g</command>
    </action>
  </item>
  <item label="Log Out">
    <action name="Exit">
      <prompt>yes</prompt>
    </action>
  </item>
</menu>

</openbox_menu>
