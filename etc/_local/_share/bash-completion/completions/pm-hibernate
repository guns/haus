# bash completion for pm-utils                             -*- shell-script -*-

_pm_action()
{
    local cur prev words cword
    _init_completion || return

    COMPREPLY=( $( compgen -W "--help $( _parse_help "$1" )" -- "$cur" ) )
} &&
complete -F _pm_action pm-hibernate pm-suspend pm-suspend-hybrid

# ex: filetype=sh
