# mailman check_db completion                              -*- shell-script -*-

_check_db()
{
    local cur prev words cword
    _init_completion || return

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $( compgen -W '--all --verbose --help' -- "$cur" ) )
    else
        _xfunc list_lists _mailman_lists
    fi

} &&
complete -F _check_db check_db

# ex: filetype=sh
