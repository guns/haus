# munindoc completion                                      -*- shell-script -*-

_munindoc()
{
    local cur prev words cword
    _init_completion || return

    COMPREPLY=( $( compgen -W \
        '$( command ls /usr/share/munin/plugins 2>/dev/null )' -- "$cur" ) )
} &&
complete -F _munindoc munindoc

# ex: filetype=sh
