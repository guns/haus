# pwck(8) completion                                       -*- shell-script -*-

_pwck()
{
    local cur prev words cword
    _init_completion || return

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $( compgen -W '$( _parse_usage "$1" )' -- "$cur" ) )
        return
    fi

    _filedir
} &&
complete -F _pwck pwck

# ex: filetype=sh
