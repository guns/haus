# pwdx(1) completion                                       -*- shell-script -*-

_pwdx()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        -h|--help|-V|--version)
            return
            ;;
    esac

    if [[ $cur == -* ]]; then
        local help='$( _parse_help "$1" )'
        [[ $help ]] || help=-V
        COMPREPLY=( $( compgen -W "$help" -- "$cur" ) )
    else
        _pids
    fi
} &&
complete -F _pwdx pwdx

# ex: filetype=sh
