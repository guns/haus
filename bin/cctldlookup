def fmt k, v
  "%s: %s%s%s" % [
    k,
    v[0],
    (" (#{v[1]})" if v[1].size > 0),
    ("\n     # " << v[2] if v[2].size > 0)
  ]
end

def lookup k
  if v = CCTLD[k]
    puts fmt(k, v)
    return true
  end
  false
end

def reverse_lookup pat
  CCTLD.each do |(k, v)|
    if v.join(' ') =~ pat
      puts fmt(k, v)
      return true
    end
  end
  false
end

ARGV.each do |a|
  catch :done do
    [a, a.downcase, '.' << a.downcase].each do |k|
      lookup k and throw :done
    end
    reverse_lookup Regexp.compile(a, 'i')
  end
end
