require 'optparse'
require 'nokogiri'

URL = 'https://techblog.willshouse.com/2012/01/03/most-common-user-agents/'
AGENT = 'Mozilla/5.0 (Macintosh; Intel Mac OS X 10_11_3) AppleWebKit/601.4.4 (KHTML, like Gecko) Version/9.0.3 Safari/601.4.4'
html = IO.popen(['curl', '--user-agent', AGENT, '--silent', '--location', URL]) { |io| io.read }

puts Nokogiri::HTML.parse(html).css('textarea.get-the-list').text
